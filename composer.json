    "description": "Import Pricelist from Discomp to AbraFlexi",
    "type": "package",
    "require": {
        "spojenet/flexibee": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.86",
        "ergebnis/composer-normalize": "^2.47",
        "ergebnis/php-cs-fixer-config": "^6.53"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SpojeNet\\Discomp\\": "src/Discomp"
        }
    },
    "authors": [
        {
            "name": "CyberVitexus",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
